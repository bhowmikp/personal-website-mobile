{
	"mx-auto": {
		"style": {
			"marginLeft": "auto",
			"marginRight": "auto"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"h-screen": {
		"style": {
			"height": "100vh"
		}
	},
	"w-1/2": {
		"style": {
			"width": "50%"
		}
	},
	"flex-row": {
		"style": {
			"flexDirection": "row"
		}
	},
	"flex-col": {
		"style": {
			"flexDirection": "column"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"bg-red-600": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(220 38 38 / var(--tw-bg-opacity))"
		}
	},
	"bg-yellow-700": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(161 98 7 / var(--tw-bg-opacity))"
		}
	},
	"text-[30px]": {
		"style": {
			"fontSize": 30
		}
	},
	"text-blue-800": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(30 64 175 / var(--tw-text-opacity))"
		}
	}
}